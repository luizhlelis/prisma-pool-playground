{
  "name": "nestjs-playground",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "TS_NODE_PROJECT=tsconfig.paths.json node -r tsconfig-paths/register dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration --testMatch='**/test/integration/**/*.(e2e-spec|spec).ts'",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest test/integration"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.202.0",
    "@opentelemetry/auto-instrumentations-node": "^0.60.1",
    "@opentelemetry/exporter-logs-otlp-grpc": "^0.202.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.202.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.202.0",
    "@opentelemetry/instrumentation": "^0.202.0",
    "@opentelemetry/instrumentation-mysql2": "^0.48.0",
    "@opentelemetry/instrumentation-pg": "^0.54.0",
    "@opentelemetry/instrumentation-redis": "^0.49.1",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-logs": "^0.202.0",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.202.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.34.0",
    "@prisma/client": "^6.10.1",
    "@prisma/instrumentation": "^6.11.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "module-alias": "^2.2.3",
    "prisma": "^6.10.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s",
      "!src/**/*.spec.ts",
      "!src/**/*.e2e-spec.ts"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/**/*.(spec|e2e-spec).ts"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "_moduleAliases": {
    "@": "dist/src",
    "@controllers": "dist/src/controllers",
    "@handlers": "dist/src/handlers",
    "@commands": "dist/src/commands",
    "@queries": "dist/src/queries",
    "@models": "dist/src/models",
    "@dtos": "dist/src/dtos",
    "@exceptions": "dist/src/exceptions",
    "@repositories": "dist/src/repositories",
    "@services": "dist/src/services",
    "@prisma": "dist/src/prisma",
    "@instrumentation": "dist/src/instrumentation",
    "@config": "dist/src/config",
    "@common": "dist/src/common"
  }
}
